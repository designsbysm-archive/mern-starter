{
  "name": "@designsbysm/mock-server",
  "app": {
    "title": "Mock Server",
    "initials": "MS"
  },
  "version": "0.1.0",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/pro-regular-svg-icons": "^5.12.0",
    "@fortawesome/pro-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@hapi/boom": "^8.0.1",
    "JSONStream": "^1.3.5",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "customize-cra": "^0.9.1",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "memorystore": "^1.6.1",
    "mongoose": "^5.8.3",
    "mongoose-timestamp": "^0.6.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "on-finished": "^2.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.0"
  },
  "devDependencies": {
    "@babel/node": "^7.7.7",
    "eslint": "6.8.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.1",
    "stylelint-config-sass-guidelines": "^6.2.0"
  },
  "scripts": {
    "client": "react-app-rewired start",
    "db:backup": "dbTools --mongo --backup --db mockServer",
    "db:restore": "dbTools --mongo --restore --db mockServer",
    "db:grab": "dbTools --mongo --transfer --db mockServer --source prod --target local",
    "server": "nodemon --exec babel-node ./src/server",
    "build": "run-p build:*",
    "build:client": "react-app-rewired build",
    "build:server": "babel src/server -d build/server",
    "lint": "run-s lint:prettier lint:js lint:scss",
    "lint:js": "eslint --fix './src/**/*.{js,jsx}'",
    "lint:prettier": "prettier --write './src/**/*.{css,js,jsx,scss}'",
    "lint:scss": "stylelint --fix './src/**/*.{css,scss}'",
    "prebuild": "rm -rf build",
    "start": "run-p server client"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "nodemonConfig": {
    "delay": "1000"
  },
  "proxy": "http://localhost:5000/"
}
