{
  "name": "@designsbysm/mock-server",
  "app": {
    "title": "Mock Server",
    "initials": "MS"
  },
  "version": "0.1.0",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/pro-regular-svg-icons": "^5.8.1",
    "@fortawesome/pro-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "JSONStream": "^1.3.5",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.4",
    "boom": "^7.3.0",
    "customize-cra": "^0.2.12",
    "date-fns": "^2.0.0-alpha.27",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "memorystore": "^1.6.1",
    "mongoose": "^5.5.2",
    "mongoose-timestamp": "^0.6.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "on-finished": "^2.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.1",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.8",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "eslint": "5.12.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "stylelint": "^10.0.1",
    "stylelint-config-sass-guidelines": "^5.4.0"
  },
  "scripts": {
    "client": "react-app-rewired start",
    "db:backup": "dbTools --mongo --backup --db mockServer",
    "db:restore": "dbTools --mongo --restore --db mockServer",
    "db:grab:prod": "dbTools --mongo --transfer --db mockServer --source prod --target local",
    "server": "nodemon --exec babel-node ./src/server",
    "build": "run-p build:*",
    "build:client": "react-app-rewired build",
    "build:server": "babel src/server -d build/server",
    "lint": "run-s lint:prettier lint:js lint:scss",
    "lint:js": "eslint --fix './src/**/*.{js,jsx}'",
    "lint:prettier": "prettier --write './src/**/*.{js,jsx}'",
    "lint:scss": "stylelint --fix './src/**/*.{css,scss}'",
    "prebuild": "rm -rf build",
    "start": "run-p server client"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "nodemonConfig": {
    "delay": "1000"
  },
  "proxy": "http://localhost:5000/"
}
